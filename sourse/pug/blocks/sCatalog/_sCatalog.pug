				+sortingItem(`II КАТЕГОРИЯ`)
				+sortingItem(`КАТЕГОРИЯ ПРЕМИУМ`)
			+e.card-row.row
				- const wrapClass="col-12 col-sm-6 col-md-4 col-lg-3"
