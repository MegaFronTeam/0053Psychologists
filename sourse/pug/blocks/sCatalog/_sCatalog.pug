mixin sCatalog(data)
	// start sCatalog
	+b.sCatalog.section&attributes(attributes)
		.container
			+e.input-wrap
				p Сортировка:
				+input("По-возрасту",'select','')
			+e.row.row.sorting-row
				+sortingItem(`ВСЕ`).active
				+sortingItem(`БАЗОВАЯ КАТЕГОРИЯ`)
